{
  "name": "phone_catalog_app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/index.ts",
    "start": "node ./build/index.js",
    "prod": "npm run build && npm run start",
    "lint": "tsc --noEmit && eslint src",
    "lint:fix": "tsc --noEmit && eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "db:create": "docker create -p 27017:27017 --name pepephone mongo",
    "db:remove": "docker rm -f -v pepephone",
    "db:start": "docker start pepephone",
    "db:stop": "docker stop pepephone"
  },
