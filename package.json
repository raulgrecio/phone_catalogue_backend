    "mongodb": "^3.6.3",
    "mongoose": "^5.10.13",
    "morgan": "^1.10.0",
    "node-mongodb-fixtures": "^3.2.7"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
